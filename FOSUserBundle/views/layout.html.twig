<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <link rel="stylesheet" href="{{ asset('bundles/webshop/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/webshop/Admin_Panel/css/AdminLTE.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/webshop/css/main.css') }}">
    </head>
    <body>
        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}

        <div>
            <div class="fosuser-form text-center">
                <a href="/">
                    <img width="200" src="{{ asset('bundles/webshop/images/logo/logo.png') }}" alt="logo images">
                </a>
            </div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
    </body>
</html>
